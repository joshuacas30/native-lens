      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.nativelens",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA_kdnKlO3iMBmcaidkomXf19qpueQ1aPI"
        }
      },
      "networkSecurityConfig": {
        "usescleartextTraffic": true,
        "domainConfigs": [
          {
            "domains": ["13.236.119.181"],
            "includeSubdomains": true
          }
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "react-native-vision-camera"
